    "@tanstack/react-query": "^5.62.8",
    "@tanstack/react-query-devtools": "^5.62.8",
    "@xstate/react": "^5.0.0",
    "date-fns": "^4.1.0",
    "i18next": "^24.2.0",
    "i18next-fetch-backend": "^6.0.0",
    "next": "^15.1.2",
    "nextjs-toploader": "^3.7.15",
    "nuqs": "^2.2.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^4.1.2",
    "react-i18next": "^15.2.0",
    "remeda": "^2.18.0",
    "xstate": "^5.19.0"
